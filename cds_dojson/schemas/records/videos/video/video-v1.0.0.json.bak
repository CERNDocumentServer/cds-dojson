{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "required": [
    "category",
    "contributors",
    "copyright",
    "date",
    "description",
    "duration",
    "language",
    "license",
    "publication_date",
    "recid",
    "report_number",
    "title",
    "type"
  ],
  "type": "object",
  "properties": {
    "contributors": {
      "minItems": 1,
      "items": {
        "additionalProperties": false,
        "required": [
          "name"
        ],
        "type": "object",
        "description": "An entity responsible for making contributions to the resource. Examples of a Contributor include a person, an organization, or a service. Typically, the name of a Contributor should be used to indicate the entity.",
        "properties": {
          "name": {
            "type": "string",
            "description": "Full name of person. Personal name format: family, given."
          },
          "ids": {
            "uniqueItems": true,
            "items": {
              "additionalProperties": false,
              "type": "object",
              "properties": {
                "source": {
                  "type": "string"
                },
                "value": {
                  "type": "string"
                }
              }
            },
            "type": "array",
            "description": "List of IDs related with the person."
          },
          "affiliations": {
            "uniqueItems": true,
            "items": {
              "type": "string"
            },
            "type": "array",
            "description": "Affiliation(s) for the purpose of this specific record."
          },
          "role": {
            "enum": [
              "Animations by",
              "Camera Operator",
              "Comments by",
              "Co-Producer",
              "Creator",
              "Credits",
              "Director",
              "Editor",
              "Music by",
              "Narrator",
              "Photography",
              "Producer",
              "Reporter",
              "Screenwriter",
              "Speaker",
              "Translator"
            ],
            "type": "string"
          },
          "contribution": {
            "type": "string",
            "description": "Specific contribution of the person to this specific record."
          },
          "email": {
            "type": "string",
            "description": "Contact email for the purpose of this specific record.",
            "format": "email"
          }
        }
      },
      "type": "array"
    },
    "report_number": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "physical_medium": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "shelf": {
            "type": "string"
          },
          "copy_number": {
            "type": "string"
          },
          "arrangement": {
            "type": "string"
          },
          "note": {
            "type": "string"
          },
          "camera": {
            "type": "string"
          },
          "location": {
            "type": "string"
          },
          "bar_code": {
            "type": "string"
          },
          "medium_standard": {
            "type": "string"
          },
          "sequence_number": {
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "internal_note": {
            "type": "string"
          }
        }
      },
      "type": "array",
      "description": "Physical medium, typically used for archive resources.",
      "title": "Physical medium"
    },
    "_cds": {
      "type": "object",
      "properties": {
        "state": {
          "type": "object",
          "properties": {
            "file_upload": {
              "enum": [
                "PENDING",
                "RECEIVED",
                "STARTED",
                "SUCCESS",
                "FAILURE",
                "REVOKED",
                "RETRY",
                "IGNORED",
                "REJECTED"
              ],
              "type": "string"
            },
            "file_download": {
              "enum": [
                "PENDING",
                "RECEIVED",
                "STARTED",
                "SUCCESS",
                "FAILURE",
                "REVOKED",
                "RETRY",
                "IGNORED",
                "REJECTED"
              ],
              "type": "string"
            },
            "file_transcode": {
              "enum": [
                "PENDING",
                "RECEIVED",
                "STARTED",
                "SUCCESS",
                "FAILURE",
                "REVOKED",
                "RETRY",
                "IGNORED",
                "REJECTED"
              ],
              "type": "string"
            },
            "file_video_extract_frames": {
              "enum": [
                "PENDING",
                "RECEIVED",
                "STARTED",
                "SUCCESS",
                "FAILURE",
                "REVOKED",
                "RETRY",
                "IGNORED",
                "REJECTED"
              ],
              "type": "string"
            },
            "file_video_metadata_extraction": {
              "enum": [
                "PENDING",
                "RECEIVED",
                "STARTED",
                "SUCCESS",
                "FAILURE",
                "REVOKED",
                "RETRY",
                "IGNORED",
                "REJECTED"
              ],
              "type": "string"
            }
          }
        },
        "modified_by": {
          "type": "integer"
        },
        "extracted_metadata": {
          "type": "object"
        }
      }
    },
    "keywords": {
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          }
        }
      },
      "type": "array"
    },
    "featured": {
      "type": "boolean"
    },
    "_access": {
      "type": "object",
      "properties": {
        "read": {
          "items": {
            "type": "string",
            "title": "Read"
          },
          "type": "array"
        },
        "update": {
          "items": {
            "type": "string",
            "title": "Update"
          },
          "type": "array"
        }
      }
    },
    "duration": {
      "type": "string"
    },
    "external_system_identifiers": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "value": {
            "type": "string"
          },
          "schema": {
            "type": "string"
          }
        }
      },
      "type": "array",
      "description": "List of identifiers on external systems.",
      "title": "External identifiers"
    },
    "subject": {
      "additionalProperties": false,
      "description": "Subject.",
      "type": "object",
      "properties": {
        "source": {
          "type": "string"
        },
        "term": {
          "type": "string"
        }
      },
      "title": "Subject"
    },
    "category": {
      "type": "string",
      "description": "Category of the project."
    },
    "related_links": {
      "uniqueItems": true,
      "items": {
        "additionalProperties": false,
        "type": "object",
        "properties": {
          "url": {
            "title": "Related URL",
            "type": "string",
            "format": "url"
          },
          "name": {
            "type": "string",
            "title": "Related link name"
          }
        },
        "title": "Related links"
      },
      "type": "array",
      "title": "Related links information"
    },
    "agency_code": {
      "type": "string"
    },
    "copyright": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string",
          "format": "url"
        },
        "holder": {
          "type": "string",
          "description": "The party holding the legal copyright to the record."
        },
        "year": {
          "type": "string",
          "description": "The year during which the claimed copyright for the CreativeWork was first asserted.",
          "format": "date-time"
        }
      }
    },
    "title": {
      "additionalProperties": false,
      "required": [
        "title"
      ],
      "type": "object",
      "description": "A name given to the resource. Typically, a Title will be a name by which the resource is formally known.",
      "properties": {
        "subtitle": {
          "type": "string"
        },
        "title": {
          "type": "string"
        }
      }
    },
    "_files": {
      "items": {
        "type": "object",
        "description": "A file object described using some basic subfields. (Usually to be extended).",
        "properties": {
          "bucket_id": {
            "type": "string"
          },
          "checksum": {
            "type": "string"
          },
          "completed": {
            "type": "boolean"
          },
          "content_type": {
            "type": "string"
          },
          "context_type": {
            "enum": [
              "frame",
              "frames-preview",
              "subtitle",
              "master",
              "subformat",
              "playlist",
              "poster"
            ],
            "type": "string"
          },
          "file_id": {
            "type": "string"
          },
          "frame": {
            "type": "array"
          },
          "frames-preview": {
            "type": "array"
          },
          "key": {
            "type": "string"
          },
          "media_type": {
            "type": "string"
          },
          "playlist": {
            "type": "array"
          },
          "size": {
            "type": "integer"
          },
          "subformat": {
            "type": "array"
          },
          "version_id": {
            "type": "string"
          }
        }
      },
      "type": "array"
    },
    "note": {
      "type": "string",
      "description": "Note.",
      "title": "Note"
    },
    "location": {
      "type": "string",
      "description": "Place or location.",
      "title": "Place or location"
    },
    "_deposit": {
      "type": "object",
      "properties": {
        "pid": {
          "type": "object",
          "properties": {
            "revision_id": {
              "type": "integer"
            },
            "type": {
              "type": "string"
            },
            "value": {
              "type": "string"
            }
          }
        },
        "owners": {
          "items": {
            "type": "integer"
          },
          "title": "Owners",
          "type": "array",
          "description": "Owners of the record (a list of user's IDs).",
          "name": "owners"
        },
        "status": {
          "enum": [
            "published",
            "draft"
          ],
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "created_by": {
          "type": "integer"
        }
      }
    },
    "type": {
      "type": "string",
      "description": "Type of the project."
    },
    "description": {
      "type": "string",
      "description": "An account of the resource. Description may include but is not limited to: an abstract, a table of contents, a graphical representation, or a free-text account of the resource."
    },
    "_oai": {
      "additionalProperties": false,
      "type": "object",
      "description": "OAI-PMH specific information.",
      "properties": {
        "updated": {
          "type": "string",
          "description": "Last OAI-PMH set update (ISO8601 formatted timestamp)"
        },
        "id": {
          "type": "string",
          "description": "OAI-PMH record identifier."
        },
        "sets": {
          "items": {
            "type": "string"
          },
          "type": "array",
          "description": "OAI-PMH sets for record."
        }
      }
    },
    "translations": {
      "items": {
        "type": "object",
        "properties": {
          "description": {
            "type": "string",
            "description": "An account of the resource. Description may include but is not limited to: an abstract, a table of contents, a graphical representation, or a free-text account of the resource."
          },
          "language": {
            "default": "en",
            "enum": [
              "ar",
              "bg",
              "ca",
              "ch",
              "cs",
              "da",
              "de",
              "el",
              "en-fr",
              "en",
              "es",
              "fi",
              "fr",
              "hr",
              "hu",
              "it",
              "ja",
              "ka",
              "ko",
              "nl",
              "no",
              "pl",
              "pt",
              "ru",
              "silent",
              "sk",
              "sr",
              "sv",
              "tr",
              "uk",
              "zh_CN",
              "zh_TW",
              "zh"
            ],
            "type": "string",
            "description": "A language of the resource."
          },
          "title": {
            "additionalProperties": false,
            "required": [
              "title"
            ],
            "type": "object",
            "description": "A name given to the resource. Typically, a Title will be a name by which the resource is formally known.",
            "properties": {
              "subtitle": {
                "type": "string"
              },
              "title": {
                "type": "string"
              }
            }
          }
        }
      },
      "type": "array"
    },
    "date": {
      "type": "string",
      "description": "A point or period of time associated with an event in the lifecycle of the resource. (http://www.w3.org/TR/NOTE-datetime)",
      "format": "date-time"
    },
    "publication_date": {
      "title": "Publication date",
      "type": "string",
      "description": "When the record is published",
      "format": "date-time"
    },
    "internal_note": {
      "type": "string",
      "description": "Internal note.",
      "title": "Internal note"
    },
    "internal_categories": {
      "description": "FIXME: temporary location for potential future communities",
      "type": "object",
      "properties": {
        "Press": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "Press": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "doi": {
      "type": "string",
      "description": "Record DOI identifier (string)."
    },
    "license": {
      "uniqueItems": true,
      "items": {
        "additionalProperties": false,
        "type": "object",
        "properties": {
          "url": {
            "title": "URL of the license",
            "type": "string",
            "format": "url"
          },
          "credit": {
            "type": "string",
            "title": "Credit"
          },
          "material": {
            "type": "string",
            "title": "Material"
          },
          "license": {
            "type": "string",
            "title": "License name"
          }
        },
        "title": "License"
      },
      "type": "array",
      "title": "License information"
    },
    "language": {
      "description": "A language of the resource.",
      "default": "en",
      "enum": [
        "ar",
        "bg",
        "ca",
        "ch",
        "cs",
        "da",
        "de",
        "el",
        "en-fr",
        "en",
        "es",
        "fi",
        "fr",
        "hr",
        "hu",
        "it",
        "ja",
        "ka",
        "ko",
        "nl",
        "no",
        "pl",
        "pt",
        "ru",
        "silent",
        "sk",
        "sr",
        "sv",
        "tr",
        "uk",
        "zh_CN",
        "zh_TW",
        "zh"
      ],
      "type": "string"
    },
    "accelerator_experiment": {
      "additionalProperties": false,
      "description": "Accelerator and experiment information.",
      "type": "object",
      "properties": {
        "project": {
          "type": "string"
        },
        "study": {
          "type": "string"
        },
        "experiment": {
          "type": "string"
        },
        "accelerator": {
          "type": "string"
        },
        "facility": {
          "type": "string"
        }
      },
      "title": "Accelerator/Experiment"
    },
    "audio_characteristics": {
      "type": "object",
      "properties": {
        "playback_channels": {
          "type": "string"
        }
      }
    },
    "vr": {
      "type": "boolean"
    },
    "recid": {
      "type": "number",
      "description": "Invenio record identifier (integer)."
    },
    "original_source": {
      "type": "string"
    },
    "_project_id": {
      "type": "string"
    }
  },
  "title": "CDS Base Record Schema v1.0.0"
}
